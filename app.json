    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-community-flipper",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a7be8f64-9287-4a2a-a4b0-42c1bed52e12"
      }
    }
  }
}
