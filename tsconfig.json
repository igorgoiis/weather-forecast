{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@theme/*": [
        "./src/theme/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  }
}
